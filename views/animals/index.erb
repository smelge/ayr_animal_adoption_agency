Main Page for pets

<table>
  <tr>
    <td colspan="5">New Pet!</td>
    <td>
      <form action="/animals/new" method="GET">
        <input class="" type="submit" value="Add Pet to List" />
      </form>
    </td>
  </tr>
  <tr>
    <th>Image</th>
    <th>Name</th>
    <th>Species</th>
    <th>Age</th>
    <th>Adopt!</th>
    <th>"Send Away"</th>
  </tr>
  <% for pet in @pets %>
    <tr>
      <td><img src='/assets/<%= pet.photo %>' alt='<%= pet.species %>'/></td>
      <td><%= pet.name %></td>
      <td><%= pet.species %></td>
      <td><%= pet.age %></td>
      <td>
        <form action="/animals/<%= pet.id %>/edit" method="POST">
          <input class="" type="submit" value="Edit" />
        </form>
      </td>
      <td>
        <form action="/animals/<%= pet.id %>/delete" method="POST">
          <input class="" type="submit" value="Unalive" />
        </form>
      </td>
    </tr>
  <% end %>
</table>
